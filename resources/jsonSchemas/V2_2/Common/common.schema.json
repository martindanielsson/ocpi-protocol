{
    "title": "V2.2\\Common",
    "definitions": {
        "profile_type": {
            "type": "string",
            "enum": [
                "CHEAP",
                "FAST",
                "GREEN",
                "REGULAR"
            ]
        },
        "auth_method": {
            "type": "string",
            "enum": [
                "AUTH_REQUEST",
                "COMMAND",
                "WHITELIST"
            ]
        },
        "connector_type": {
            "type": "string",
            "enum": [
                "CHADEMO",
                "CHAOJI",
                "DOMESTIC_A",
                "DOMESTIC_B",
                "DOMESTIC_C",
                "DOMESTIC_D",
                "DOMESTIC_E",
                "DOMESTIC_F",
                "DOMESTIC_G",
                "DOMESTIC_H",
                "DOMESTIC_I",
                "DOMESTIC_J",
                "DOMESTIC_K",
                "DOMESTIC_L",
                "DOMESTIC_M",
                "DOMESTIC_N",
                "DOMESTIC_O",
                "GBT_AC",
                "GBT_DC",
                "IEC_60309_2_single_16",
                "IEC_60309_2_three_16",
                "IEC_60309_2_three_32",
                "IEC_60309_2_three_64",
                "IEC_62196_T1",
                "IEC_62196_T1_COMBO",
                "IEC_62196_T2",
                "IEC_62196_T2_COMBO",
                "IEC_62196_T3A",
                "IEC_62196_T3C",
                "NEMA_5_20",
                "NEMA_6_30",
                "NEMA_6_50",
                "NEMA_10_30",
                "NEMA_10_50",
                "NEMA_14_30",
                "NEMA_14_50",
                "PANTOGRAPH_BOTTOM_UP",
                "PANTOGRAPH_TOP_DOWN",
                "TESLA_R",
                "TESLA_S"
            ]
        },
        "connector_format": {
            "type": "string",
            "enum": [
                "SOCKET",
                "CABLE"
            ]
        },
        "power_type": {
            "type": "string",
            "enum": [
                "AC_1_PHASE",
                "AC_2_PHASE",
                "AC_2_PHASE_SPLIT",
                "AC_3_PHASE",
                "DC"
            ]
        },
        "price": {
            "type": "object",
            "properties": {
                "excl_vat": {
                    "type": "number"
                },
                "incl_vat": {
                    "type": [
                        "number",
                        "null"
                    ]
                }
            },
            "required": [
                "excl_vat"
            ]
        },
        "cdr_token": {
            "type": "object",
            "properties": {
                "uid": {
                    "type": "string",
                    "min_length": 1,
                    "max_length": 36
                },
                "type": {
                    "$ref": "#/definitions/token_type"
                },
                "contract_id": {
                    "type": "string",
                    "min_length": 1,
                    "max_length": 36
                }
            },
            "required": [
                "uid",
                "type",
                "contract_id"
            ]
        },
        "token_type": {
            "type": "string",
            "enum": [
                "AD_HOC_USER",
                "APP_USER",
                "OTHER",
                "RFID"
            ]
        },
        "status_code": {
            "type": "integer"
        },
        "status_message": {
            "type": [
                "string",
                "null"
            ]
        },
        "timestamp": {
            "type": "string",
            "pattern": "^([0-9]{4})-(1[0-2]|0[0-9])-([0-2][0-9]|3[0-1])T([0-1][0-9]|2[0-4]):[0-5][0-9]:[0-5][0-9](Z|[-+](0[0-9]|1[0-4]):[0-5][0-9])?$"
        },
        "business_details": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 100
                },
                "website": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "format": "uri"
                },
                "logo": {
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "$ref":"#/definitions/image"
                        }
                    ]
                }
            },
            "required": [
                "name"
            ],
            "additionalProperties": false
        },
        "image": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string",
                    "format": "uri"
                },
                "thumbnail": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "format": "uri"
                },
                "category": {
                    "type": "string",
                    "enum": [
                        "CHARGER",
                        "ENTRANCE",
                        "LOCATION",
                        "NETWORK",
                        "OPERATOR",
                        "OTHER",
                        "OWNER"
                    ]
                },
                "type": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 4
                },
                "width": {
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "height": {
                    "type": [
                        "integer",
                        "null"
                    ]
                }
            },
            "required": [
                "url",
                "category",
                "type"
            ],
            "additionalProperties": false
        },
        "latitude": {
            "type": "string",
            "minLength": 1,
            "maxLength": 10,
            "pattern": "^-?[0-9]{1,2}\\.[0-9]{5,7}$"
        },
        "longitude": {
            "type": "string",
            "minLength": 1,
            "maxLength": 11,
            "pattern": "^-?[0-9]{1,3}\\.[0-9]{5,7}$"
        },
        "geo_location": {
            "type": "object",
            "properties": {
                "latitude": {
                    "$ref": "#/definitions/latitude"
                },
                "longitude": {
                    "$ref": "#/definitions/longitude"
                }
            },
            "additionalProperties": false
        },
        "display_text": {
            "type": "object",
            "properties": {
                "language": {
                    "type": "string",
                    "minLength": 2,
                    "maxLength": 2,
                    "pattern": "^[A-Za-z]{2}$"
                },
                "text": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 512
                }
            },
            "required": [
                "language",
                "text"
            ],
            "additionalProperties": false
        },
        "energy_mix": {
            "type": "object",
            "properties": {
                "is_green_energy": {
                    "type": "boolean"
                },
                "energy_sources": {
                    "type": [
                        "array",
                        "null"
                    ],
                    "items": {
                        "type": "object",
                        "properties": {
                            "source": {
                                "type": "string",
                                "enum": [
                                    "NUCLEAR",
                                    "GENERAL_FOSSIL",
                                    "COAL",
                                    "GAS",
                                    "GENERAL_GREEN",
                                    "SOLAR",
                                    "WIND",
                                    "WATER"
                                ]
                            },
                            "percentage": {
                                "type": "number",
                                "minimum": 0,
                                "maximum": 100
                            }
                        },
                        "required": [
                            "source",
                            "percentage"
                        ],
                        "additionalProperties": false
                    }
                },
                "environ_impact": {
                    "type": [
                        "array",
                        "null"
                    ],
                    "items": {
                        "type": "object",
                        "properties": {
                            "category": {
                                "type": "string",
                                "enum": [
                                    "NUCLEAR_WASTE",
                                    "CARBON_DIOXIDE"
                                ]
                            },
                            "amount": {
                                "type": "number"
                            }
                        },
                        "required": [
                            "category",
                            "amount"
                        ],
                        "additionalProperties": false
                    }
                },
                "supplier_name": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "minLength": 1,
                    "maxLength": 64
                },
                "energy_product_name": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "minLength": 1,
                    "maxLength": 64
                }
            },
            "required": [
                "is_green_energy"
            ],
            "additionalProperties": false
        },
        "role": {
            "type": "string",
            "enum": [
                "CPO",
                "EMSP",
                "HUB",
                "NAP",
                "NSP",
                "OTHER",
                "SCSP"
            ]
        },
        "charging_period": {
            "type": "object",
            "properties": {
                "start_date_time": {
                    "$ref": "common.schema.json#/definitions/timestamp"
                },
                "dimensions": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "string",
                                "enum": [
                                    "CURRENT",
                                    "ENERGY",
                                    "ENERGY_EXPORT",
                                    "ENERGY_IMPORT",
                                    "MAX_CURRENT",
                                    "MIN_CURRENT",
                                    "MAX_POWER",
                                    "MIN_POWER",
                                    "PARKING_TIME",
                                    "POWER",
                                    "RESERVATION_TIME",
                                    "STATE_OF_CHARGE",
                                    "TIME"
                                ]
                            },
                            "volume": {
                                "type": "number"
                            }
                        },
                        "required": [
                            "type",
                            "volume"
                        ],
                        "additionalProperties": false
                    }
                },
                "tariff_id": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "minLength": 1,
                    "maxLength": 36
                }
            },
            "required": [
                "start_date_time",
                "dimensions"
            ],
            "additionalProperties": false
        },
        "location_references": {
            "type": "object",
            "properties": {
                "location_id": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 36
                },
                "evse_uids": {
                    "type": [
                        "array",
                        "null"
                    ],
                    "items": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 36
                    }
                }
            },
            "required": [
                "location_id"
            ]
        },
        "version": {
            "type": "string",
            "enum": [
                "2.0",
                "2.1",
                "2.1.1",
                "2.2",
                "2.2.1"
            ]
        }
    }
}